{
  "name": "@1wayseo/blog-sdk",
  "version": "1.0.0",
  "description": "SDK for integrating 1waySEO blog content into your website",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "1wayseo",
    "blog",
    "cms",
    "sdk",
    "api-client"
  ],
  "author": "1waySEO",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhenheco/autoseo.git",
    "directory": "packages/blog-sdk"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
