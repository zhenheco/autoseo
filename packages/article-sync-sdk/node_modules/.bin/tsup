#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/500G/Claudecode/Z-1wayseo/packages/article-sync-sdk/node_modules/.pnpm/tsup@8.5.1_typescript@5.9.3/node_modules/tsup/dist/node_modules:/Volumes/500G/Claudecode/Z-1wayseo/packages/article-sync-sdk/node_modules/.pnpm/tsup@8.5.1_typescript@5.9.3/node_modules/tsup/node_modules:/Volumes/500G/Claudecode/Z-1wayseo/packages/article-sync-sdk/node_modules/.pnpm/tsup@8.5.1_typescript@5.9.3/node_modules:/Volumes/500G/Claudecode/Z-1wayseo/packages/article-sync-sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/500G/Claudecode/Z-1wayseo/packages/article-sync-sdk/node_modules/.pnpm/tsup@8.5.1_typescript@5.9.3/node_modules/tsup/dist/node_modules:/Volumes/500G/Claudecode/Z-1wayseo/packages/article-sync-sdk/node_modules/.pnpm/tsup@8.5.1_typescript@5.9.3/node_modules/tsup/node_modules:/Volumes/500G/Claudecode/Z-1wayseo/packages/article-sync-sdk/node_modules/.pnpm/tsup@8.5.1_typescript@5.9.3/node_modules:/Volumes/500G/Claudecode/Z-1wayseo/packages/article-sync-sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-default.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-default.js" "$@"
fi
